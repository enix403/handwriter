{
  "name": "nascon-prep",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "wasm-gen": "wasm-pack build wasmcore --target web --out-dir pkg --out-name generated",
    "dev:wasm": "yarn wasm-gen --dev",
    "dev": "vite",
    "build:wasm": "yarn wasm-gen --release",
    "build:client": "tsc && vite build",
    "build": "yarn build:wasm && yarn build:client",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "format": "prettier -uw --cache --ignore-path .prettierignore ."
  },
  "dependencies": {
    "@fontsource/inter": "^5.0.16",
    "@mantine/core": "^7.6.1",
    "@mantine/hooks": "^7.6.1",
    "@react-hook/size": "^2.1.2",
    "@tabler/icons-react": "^2.47.0",
    "clsx": "^2.1.0",
    "immer": "^10.0.4",
    "konva": "^9.3.6",
    "ramda": "^0.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-konva": "^18.2.10",
    "react-router-dom": "^6.22.1",
    "use-immer": "^0.9.0",
    "wasm-pack": "^0.12.1"
  },
  "devDependencies": {
    "@types/ramda": "^0.29.11",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "postcss-preset-mantine": "^1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "2",
    "prettier-plugin-tailwindcss": "0.3.0",
    "sass": "^1.71.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-tsconfig-paths": "^4.3.1"
  }
}
